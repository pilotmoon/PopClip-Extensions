	<key>Extension Identifier</key>
	<string>com.pilotmoon.popclip.extension.google-translate</string>
	<key>Extension Description</key>
	<string>Open Google Translate in a web browser with the selected text.</string>
	<key>Actions</key>
	<array>
		<dict>
			<key>URL</key>
			<string>https://{popclip option site}/?langpair=auto%7Cauto&amp;text={popclip text}</string>
