<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>vid.png</string>
			<key>Regular Expression</key>
			<string>(?s)^.{1,}$</string>
			<key>Title</key>
			<string>YouTube</string>
			<key>URL</key>
			<string>http://www.youtube.com/results?search_query={popclip text}</string>
		</dict>
	</array>
	<key>Extension Identifier</key>
	<string>com.pilotmoon.popclip.extension.youtube</string>
	<key>Extension Name</key>
	<string>YouTube</string>
	<key>Version</key>
	<integer>2</integer>
	<key>Extension Description</key>
	<string>Search for videos on YouTube.</string>
</dict>
</plist>
