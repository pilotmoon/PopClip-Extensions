<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>connectedAndReady.png</string>
			<key>Regular Expression</key>
			<string>(\+)?\(?\d{1,4}\)?(-|\s|\()?\(?\d{1,3}\)?(-|\s)?\(?\d{0,4}\)?(-|\s)?\d{2,3}?(-|\s)?\d{2,3}((-|\s)?\d{1,4})?((-|\s)\d{1,3})?\b|\b((\+)?\d{3}\.\d{3,4}\.\d{4})</string>
			<key>Title</key>
			<string>Dial with Phone Amego</string>
			<key>URL</key>
			<string>phoneamego://{popclip text};alert=no</string>
		</dict>
	</array>
	<key>Apps</key>
	<array>
		<dict>
			<key>Bundle Identifier</key>
			<string>com.sustworks.PhoneAmego</string>
			<key>Check Installed</key>
			<true/>
			<key>Link</key>
			<string>http://www.sustworks.com/pa_guide/</string>
			<key>Name</key>
			<string>Phone Amego</string>
		</dict>
	</array>
	<key>Credits</key>
	<array>
		<dict>
			<key>Name</key>
			<string>Peter Sichel</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Dial the phone number with Phone Amego.</string>
	<key>Extension Identifier</key>
	<string>com.pilotmoon.popclip.extension.PhoneAmego</string>
	<key>Extension Name</key>
	<string>Phone Amego</string>
</dict>
</plist>
