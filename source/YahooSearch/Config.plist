				<string>it.search.yahoo.com</string>			
				<string>in.search.yahoo.com</string>							
				<string>mx.search.yahoo.com</string>							
				<string>nl.search.yahoo.com</string>			
				<string>ro.search.yahoo.com</string>		
				<string>sg.search.yahoo.com</string>
				<string>tr.search.yahoo.com</string>				
