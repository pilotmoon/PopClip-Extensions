<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>Yahoo.png</string>
			<key>Regular Expression</key>
			<string>(?s)^.{1,1900}$</string>
			<key>Title</key>
			<string>Yahoo! Search</string>
			<key>URL</key>
			<string>https://{popclip option site}/search?p={popclip text}</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Search the web using Yahoo!.</string>
	<key>Extension Identifier</key>
	<string>com.pilotmoon.popclip.extension.yahoo-search</string>
	<key>Extension Name</key>
	<string>Yahoo!</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Option Identifier</key>
			<string>site</string>
			<key>Option Label</key>
			<dict>
				<key>en</key>
				<string>Yahoo! Site</string>
			</dict>
			<key>Option Type</key>
			<string>multiple</string>
			<key>Option Values</key>
			<array>
				<string>search.yahoo.com</string>
				<string>search.yahoo.co.jp</string>
				<string>br.search.yahoo.com</string>		
