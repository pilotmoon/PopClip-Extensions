<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>jira.png</string>
			<key>Regular Expression</key>
			<string>(?s)^.{1,1900}$</string>
			<key>Title</key>
			<string>Jira</string>
			<key>URL</key>
			<string>https://{popclip option site}/secure/QuickSearch.jspa?searchString={popclip text}</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Search Jira by Atlassian, with option to choose the site.</string>
	<key>Extension Identifier</key>
	<string>com.mattklepac.popclip.extension.jira</string>
	<key>Extension Name</key>
	<string>Jira</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Option Identifier</key>
			<string>site</string>
			<key>Option Label</key>
			<dict>
				<key>en</key>
				<string>Jira Site</string>
			</dict>
			<key>Option Type</key>
			<string>string</string>
			<key>Option Default Value</key>
			<string>jira.atlassian.com</string>
		</dict>
	</array>
	<key>Required Software Version</key>
	<integer>693</integer>
</dict>
</plist>
