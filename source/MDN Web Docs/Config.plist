<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>mdn.png</string>
			<key>URL</key>
			<string>https://developer.mozilla.org/search?q={popclip text}</string>
			<key>Title</key>
			<string>MDN Web Docs Search</string>
		</dict>
	</array>
	<key>Extension Identifier</key>
	<string>de.trommelspeicher.popclip.extension.mdn-web-docs</string>
	<key>Extension Name</key>
	<string>MDN Web Docs</string>
	<key>Extension Description</key>
    <string>Open Mozilla Developer Network (MDN) Web Docs in a web browser with the selected text to search.</string>
</dict>
</plist>
